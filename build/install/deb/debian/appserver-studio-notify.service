[Unit]
Description=AppServer-studio-notify
After=network.target syslog.target

[Service]
PIDFile=/run/appserver/appserver-studio-notify.pid
Type=simple
User=onlyoffice
Group=onlyoffice
WorkingDirectory=/var/www/appserver/services/ASC.Studio.Notify/
ExecStart=/usr/bin/dotnet /var/www/appserver/services/ASC.Studio.Notify/ASC.Studio.Notify.dll --urls=http://0.0.0.0:5006 --pathToConf=/etc/onlyoffice/appserver --'$STORAGE_ROOT'=/etc/onlyoffice/appserver/data --log:dir=/var/log/onlyoffice/appserver --log:name=studio-notify --core:products:folder=/var/www/appserver/products --core:products:subfolder=server  --ENVIRONMENT=production
TimeoutSec=600

Restart=no
PrivateTmp=false

[Install]
WantedBy=multi-user.target
